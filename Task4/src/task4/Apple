package task4;

public class Apple extends Fruit {
    String[] color = {"red", "yellow", "green"};
    int colorNumber;
    public Apple () {

    }
    public Apple (int number) {
        colorNumber = number;
    }
    @Override
    public String toString() {
        return "Type is Apple {" +
                "weight=" + weight + "gm" +
                ", color=" + color[colorNumber] +
                '}';
    }
}